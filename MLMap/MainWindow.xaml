<Window x:Class="MLMap.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MLMap"
        mc:Ignorable="d"
        Title="MultiLayerMap" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="294*"/>
            <ColumnDefinition Width="103*"/>
        </Grid.ColumnDefinitions>
        <Canvas Margin="10,29,10,10"/>
        <Menu Margin="0,0,0,0" Height="24" VerticalAlignment="Top"  Grid.ColumnSpan="2">
            <MenuItem Header="File" Height="24">
                <MenuItem Header="Open" />
                <MenuItem Header="Save" />
                <MenuItem Header="Save As" />
                <Separator />
                <MenuItem Header="Export" Icon=""/>
            </MenuItem>
        </Menu>
        <DockPanel Grid.Column="1" LastChildFill="False" Background="#dbdbdb" Margin="0,0,0,0"/>

    </Grid>
</Window>
